        "def open_lc(ticid,sector):\n",
        "  try:\n",
        "    lc.file = lk.open('/content/gdrive/My\\Drive/EscapeEarthData/Sec14_cleaned/{ticid}/lc.fits'.format(sector,ticid))\n",
        "    lc = lc_file.FLUX\n",
        "  except FileNotFoundError:\n",
        "   lc=none\n",
        "  pass \n",
        "  return lc\n",
        "  \n"
      ],
